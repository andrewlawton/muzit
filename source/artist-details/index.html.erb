  <!-- // end card -->
</div>
<!-- // end container-fluid -->
<div class="container-fluid">
  <div class="card">
    <div class="card-header">
      <h5>
        Activity Timeline
      </h5>
    </div>
    <div class="card-body">
      <div class="row">
        <div class="col">
          <%=image_tag( "activity-timeline.png", class: "img-fluid", alt: "Activity Timeline Image") %>
        </div>
        <!-- // end column -->
      </div>
      <!-- // end row -->
    </div>
