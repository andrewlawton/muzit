                          <li class="search-result" data-target="search-results.result" data-action="mouseover->search-results#setFocus">
                            <a href="#">Phil<span>Philipp MÃ¼ller</span></a>
                          </li>
                        </ul>
                      </li>
                      <!-- // end scroll -->
                    </ul>
                  </div>
                  <!-- // end search results -->
                </div>
                <!-- // end search -->
              </div>
              <!-- // end form-group -->
            </fieldset>
          </form>
          <div class="border px-3 py-2" id="no-titles" style="display: none;">
            <p class="p-0 m-0">
              <small>No titles have been added.</small>
            </p>
          </div>
          <table class="table border" id="list-title" style="display: none;">
            <thead>
              <tr>
                <th>Title Name</th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td class="text-truncate">
                  Phish
                </td>
                <td class="action text-right text-nowrap">
                  <%= link_to "Remove", "#", 'onclick' => 'removeListItem1()' %>
                </td>
              </tr>
            </tbody>
          </table>
          <div class="action steps text-right">
            <%= link_to 'Cancel', '../../../organization/ip-address-lists/index.html' %>
            <%= link_to 'Back', 'index.html', class: 'btn btn-outline-primary' %>
            <%= link_to 'Next', 'step3.html', class: 'btn btn-primary' %>
