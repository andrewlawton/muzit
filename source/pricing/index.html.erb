---
title: Pricing - Muzit
layout: sign-up
bodyClass: pricing
---

<header class="hero">
  <h1>
    Pricing Plans
    <span>
      Muzit plans are designed for artists, bands and labels of all sizes.
    </span>
  </h1>
</header>
<!-- // end hero -->
<section class="pricing-plans">
  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-12 text-center mb-4">
        <div class="form-group">
          <div class="btn-group" role="group" aria-label="Pricing plans">
            <button type="button" class="btn btn-outline-primary active" id="artist-pricing">Indie</button>
            <button type="button" class="btn btn-outline-primary" id="corporate-pricing">Major</button>
          </div>
          <!-- // end btn-group -->
        </div>
        <!-- // end form-group -->
      </div>
      <!-- // end column -->
    </div>
    <!-- // end row -->
    <div id="artist-plans">
      <div class="row">
        <div class="col-md-3">
          <div class="card pricing-box rounded text-center">
            <div class="card-block">
              <h4 class="card-title">
              Solo
            </h4>
            <h6 class="card-text text-nowrap">
              <sup class="currency">
                $
              </sup>
              <span class="amount">
                1,995
              </span>
              <span class="month">
                / mo
              </span>
            </h6>
            </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item text-center d-inline-block">
                1 Artist
              </li>
              <li class="list-group-item text-center d-inline-block">
                1 Account
              </li>
            </ul>
            <div class="card-block">
              <%= link_to 'Get Started', '../sign-up/index.html', class: 'btn btn-outline-primary' %>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="card pricing-box rounded text-center">
            <div class="card-block">
              <h4 class="card-title">
              Small
            </h4>
            <h6 class="card-text text-nowrap">
              <sup class="currency">
                $
              </sup>
              <span class="amount">
                2,995
              </span>
              <span class="month">
                / mo
              </span>
            </h6>
            </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item text-center d-inline-block">
                3 Artists
              </li>
              <li class="list-group-item text-center d-inline-block">
                3 Accounts
              </li>
            </ul>
            <div class="card-block">
              <%= link_to 'Get Started', '../sign-up/index.html', class: 'btn btn-outline-primary' %>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="card pricing-box rounded text-center pricing-premium">
            <div class="card-block">
              <div class="ribbon">Most Popular</div>
              <h4 class="card-title">
              Medium
            </h4>
            <h6 class="card-text text-nowrap">
              <sup class="currency">
                $
              </sup>
              <span class="amount">
                4,995
              </span>
              <span class="month">
                / mo
              </span>
            </h6>
            </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item text-center d-inline-block">
                7 Artists
              </li>
              <li class="list-group-item text-center d-inline-block">
                5 Accounts
              </li>
            </ul>
            <div class="card-block">
              <%= link_to 'Get Started', '../sign-up/index.html', class: 'btn btn-primary' %>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="card pricing-box rounded text-center">
            <div class="card-block">
              <h4 class="card-title">
              Large
            </h4>
            <h6 class="card-text text-nowrap">
              <sup class="currency">
                $
              </sup>
              <span class="amount">
                9,995
              </span>
              <span class="month">
                / mo
              </span>
            </h6>
            </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item text-center d-inline-block">
                15 Artists
              </li>
              <li class="list-group-item text-center d-inline-block">
                15 Accounts
              </li>
            </ul>
            <div class="card-block">
              <%= link_to 'Get Started', '../sign-up/index.html', class: 'btn btn-outline-primary' %>
            </div>
          </div>
        </div>
        <!-- // end column -->
      </div>
      <!-- // end row -->
    </div>
    <!-- // end artist-plans -->
    <div id="corporate-plans" style="display: none">
      <div class="row">
        <div class="col-md-4 offset-md-4">
          <div class="card pricing-box rounded text-center">
            <div class="card-block">
              <h4 class="card-title">
              Enterprise
            </h4>
            <h6 class="card-text text-nowrap">
              <sup class="currency">
                $
              </sup>
              <span class="amount">
                14,995
              </span>
              <span class="month">
                / mo
              </span>
            </h6>
            </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item text-center d-inline-block">
                25 Artists
              </li>
            </ul>
            <div class="card-block">
              <a href="#" class="btn btn-outline-primary" title="Get Started">
              Get Started
            </a>
            </div>
          </div>
        </div>
        <!-- // end column -->
      </div>
      <!-- // end row -->
    </div>
    <!-- // end corporate-plans -->
    <h2>
      Add Ons
    </h2>
    <div class="row">
      <div class="col-md-6">
        <div class="card pricing-box rounded text-center">
          <div class="card-block">
            <h4 class="card-title">
              Concierge Services
            </h4>
          </div>
          <ul class="list-group list-group-flush">
            <li class="list-group-item text-center d-inline-block">
              Custom analysis and reporting
            </li>
            <li class="list-group-item text-center d-inline-block">
              Access to a Muzit data scientist
            </li>
            <li class="list-group-item text-center d-inline-block">
              Your planning/station programming assistance
            </li>
          </ul>
          <div class="card-block">
            <a href="#" class="btn btn-outline-primary" title="Get Started">
              Contact Muzit
            </a>
          </div>
        </div>
      </div>
      <div class="col-md-6">
        <div class="card pricing-box rounded text-center">
          <div class="card-block">
            <h4 class="card-title">
              Campaigns
            </h4>
          </div>
          <ul class="list-group list-group-flush">
            <li class="list-group-item text-center d-inline-block">
              Email campaigns
            </li>
            <li class="list-group-item text-center d-inline-block">
              IP Insertion
            </li>
            <li class="list-group-item text-center d-inline-block">
              A whole lot more
            </li>
          </ul>
          <div class="card-block">
            <a href="#" class="btn btn-outline-primary" title="Get Started">
              Contact Muzit
            </a>
          </div>
        </div>
      </div>
    </div>
    <!-- // end row -->
  </div>
  <!-- // end container -->
</section>